{{! Frontpage settings orjinal column1.mustache }}
<div class="setup1">    
    <div class="modal fade" id="setup1Modal" tabindex="-1" aria-labelledby="setup1ModalLabel" aria-hidden="true">
        <div class="modal-dialog modal-lg modal-dialog-scrollable">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="setup1ModalLabel">...</h5>
                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close">
                    </button>
                </div>
                <div class="modal-body">
                    <div class= "hy-pt-1">
                        <div id="navbarHeader">
                            <div id="page-wrapper" class="container d-print-block">

                                {{{ output.standard_top_of_body_html }}}

                                <div id="page" class="container-fluid d-print-block">
                                    <div id="page-content" class="row pb-3 d-print-block">
                                        <div id="region-main-box" class="col-12">
                                            <div id="region-main">
                                                {{{ output.course_content_header }}}
                                                    {{#headercontent}}
                                                        {{> core/activity_header }}
                                                    {{/headercontent}}
                                                {{{ output.main_content }}}
                                                {{{ output.activity_navigation }}}
                                                {{{ output.course_content_footer }}}
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                {{{ output.standard_after_main_region_html }}}

                            </div>
                            {{{ output.standard_end_of_body_html }}}    
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>    

{{#js}}
M.util.js_pending('theme_boost/loader');
require(['theme_boost/loader'], function() {
    M.util.js_complete('theme_boost/loader');
});
{{/js}}
